{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2015,
        "sourceType": "module",
    },
    "extends": ["plugin:@typescript-eslint/recommend"],
    "env": {
        "node": true
    },
    "rules": {
        "indent": "off",
        "@typescript-eslint/indent": "off",
        "@typescript-eslint/explicit-function-return-type": "off"
    }
}